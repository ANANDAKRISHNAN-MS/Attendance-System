    <div class="buttons">
      <button class="Logout"><a href="../teacher_login.html">Logout</a></button>
      <button class="newCourse">Add new course</button>
    </div>
    <div class="card-content">
      
    </div>
  </div>
  <script src="../scripts/teacher_dashboard.js"></script>
</body>
</html>
